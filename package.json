    "bufferutil": "^4.0.7",
    "crypto-browserify": "^3.12.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.0.0",
    "hnswlib-node": "^1.4.2",
    "imap": "^0.8.19",
    "jszip": "^3.10.1",
    "langchain": "^0.0.54",
    "mailparser": "^3.6.4",
    "mammoth": "^1.5.1",
    "moment": "^2.29.4",
    "nodemon": "^2.0.22",
    "path-browserify": "^1.0.1",
    "pdfjs-dist": "^3.5.141",
    "process": "^0.11.10",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "stream-browserify": "^3.0.0",
    "url": "^0.11.0",
    "utf-8-validate": "^5.0.10",
    "util": "^0.12.5",
    "xml2js": "^0.5.0"
  },
